{
  "name": "devconnected",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && yarn install",
    "start": "node server.js",
    "server": "nodemon server.js --ignore client/",
    "client": "cd client && yarn start",
    "dev": "concurrently -n \"server,client\" -p \"[{name}] [{time}]\" -t \"HH:mm:ss\" \"yarn:server\" \"yarn:client\" --kill-others",
    "heroku-postbuild": "cd client && yarn && yarn run build"
  },
  "engines": {
    "node": "10.13.0",
    "yarn": "1.9.4"
  },
  "author": "ADIL",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.0.1",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.3.16",
    "normalize-url": "^4.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "rate-limit-mongo": "^1.0.3",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.8"
  }
}
