{
	"name": "devconnected",
	"version": "1.0.0",
	"main": "server.js",
	"scripts": {
		"client-install": "cd client && yarn install",
		"start": "node server.js",
		"server": "nodemon server.js --ignore client/",
		"client": "cd client && yarn start",
		"dev": "concurrently -n \"server,client\" -p \"[{name}] [{time}]\" -t \"HH:mm:ss\" \"yarn:server\" \"yarn:client\" --kill-others",
		"heroku-postbuild": "cd client && yarn && yarn run build"
	},
	"author": "ADIL",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"express-rate-limit": "^3.3.2",
		"gravatar": "^1.8.0",
		"jsonwebtoken": "^8.4.0",
		"mongoose": "^5.3.16",
		"morgan": "^1.10.0",
		"normalize-url": "^4.1.0",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"rate-limit-mongo": "^1.0.3",
		"validator": "^10.9.0"
	},
	"devDependencies": {
		"concurrently": "^5.3.0",
		"nodemon": "^1.18.8"
	}
}
